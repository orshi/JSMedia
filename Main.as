package  {	import flash.display.Sprite;		public class Main extends Sprite {			private var logoPage:LogoPage;		private var homePage:HomePage;		private var currentPage:Sprite;			public function Main() {			logoPage = new LogoPage();			addChild(logoPage);			currentPage = logoPage;			homePage = new HomePage();		}			    public function gotoHomePage():void {			removeChild(currentPage);			addChild(homePage);			currentPage = homePage;		}	}	}