package  {	import flash.events.*;		public class HomePage extends SimplePage {		public function HomePage() {			addCenterStageEvent()			addBacktoHomeEvent(bg);			addBtnOpenPageEvent(companyProfileBtn, "CompanyProfilePage");			addBtnOpenPageEvent(mainBusinessBtn, "MainBusinessPage");			addBtnOpenPageEvent(interactiveJilinBtn, "InteractiveJilinPage");			addBtnOpenPageEvent(jishiOnlineBtn, "JSOnlinePage");		}				private function addCenterStageEvent():void {			addEventListener(Event.ADDED_TO_STAGE, centerStageHandler);		}				private function centerStageHandler(event:Event):void {			this.x = stage.stageWidth / 2;			this.y = stage.stageHeight / 2;		}	}}