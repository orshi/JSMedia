package  {		import flash.display.Sprite;	import flash.events.*;		public class LogoPage extends Sprite {		public function LogoPage() {			addCenterStageEvent();			addToHomeEvent();		}			    private function addCenterStageEvent():void {			addEventListener(Event.ADDED_TO_STAGE, centerStageHandler);		}				private function addToHomeEvent():void {			addEventListener(MouseEvent.CLICK, toHomeHandler);		}				private function centerStageHandler(event:Event):void {			this.x = stage.stageWidth / 2;			this.y = stage.stageHeight / 2;		}				private function toHomeHandler(event:MouseEvent):void {			(parent as Main).gotoHomePage();		}	}	}